{
  "name": "premier-squares-webapp",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.1",
    "helmet": "^7.2.0",
    "idb": "^8.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1",
    "react-scripts": "5.0.1"
  },
  "scripts": {
    "start": "set NODE_ENV=development&& react-scripts start",
    "start:secure": "npm run build && node server.js",
    "dev:secure": "concurrently \"npm run start\" \"wait-on http://localhost:3000 && node server.js\"",
    "test": "react-scripts test",
    "test:security": "node test-security.js",
    "eject": "react-scripts eject",
    "build": "set NODE_ENV=production&& set REACT_APP_API_URL=https://z414f9tg84.execute-api.us-east-1.amazonaws.com/prod&& react-scripts build",
    "build:https": "set NODE_ENV=production&& set REACT_APP_API_URL=https://z414f9tg84.execute-api.us-east-1.amazonaws.com/prod&& set HTTPS=true&& react-scripts build",
    "serve:https": "set HTTPS=true&& set SSL_CRT_FILE=localhost.pem&& set SSL_KEY_FILE=localhost-key.pem&& react-scripts start",
    "serve:secure": "npm run build && node server.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "concurrently": "^8.2.2",
    "wait-on": "^7.2.0"
  }
}
